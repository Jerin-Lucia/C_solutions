#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    float b;
    char c;
    char d[25],e[25];
    scanf("%d \n %f \n %c \n %s ",&a,&b,&c,d);
    fgets(e,25,stdin);
    printf("Integer is %d\nFloat is %.2f\nCharacter is %c\nStrings are %s and %s ",a,b,c,d,e);  
    return 0;
}
